# Detection Feature Flags
VITE_FEATURE_USE_GPT_ONLY=true
VITE_FEATURE_USE_GPT_FIRST=false
VITE_FEATURE_USE_VISION_ONLY=false
VITE_FEATURE_SAFE_DETECT=true
VITE_DETECT_STRICT_FILTERS=true
VITE_PORTION_EST_V2=true
VITE_DETECT_STRICT_NONFOOD=true

# Generic Foods Datasource
VITE_ENABLE_GENERIC_FOODS=true

# Production safety - freeze to VISION_ONLY while debugging GPT
VITE_DETECT_MODE=VISION_ONLY
VITE_DETECT_MODE_ALLOWED=VISION_ONLY,GPT_ONLY,HYBRID

# GPT parsing mode (strict/relaxed)
VITE_GPT_PARSE_MODE=strict

# Portion v3 Feature Flags (always ON for Log mode)
VITE_PORTION_V3=true
VITE_PORTION_STRICT=false

# Diagnostic flags for GPT detection debugging
VITE_DETECT_DIAG_STRICT_ONLY=false
VITE_DETECT_DIAG_RELAX_ONLY=false