VITE_SUPABASE_PROJECT_ID="your-project-id"
VITE_SUPABASE_PUBLISHABLE_KEY="your-publishable-key"
VITE_SUPABASE_URL="https://your-project-id.supabase.co"

# Feature flags
VITE_FIX_ENRICHMENT_NO_PLACEHOLDERS=true
VITE_SCORE_ENGINE_V1=true
VITE_SHOW_PER_SERVING=true
VITE_ENGINE_V1_FIXES=true
VITE_SCANNER_THROTTLE=true

# Master switch for Take Photo
VITE_FEATURE_TAKE_PHOTO_ENABLED=true

# New photo pipeline + provider
VITE_PHOTO_PIPE_V1=true
VITE_PHOTO_OCR_PROVIDER=vision     # must be exactly 'vision'

# Optional: try one-shot barcode decode from image before OCR
VITE_PHOTO_BARCODES_ENABLE=true

# Debug only in dev/staging
VITE_DEBUG_PERF=true

# Server-side OCR configuration (for edge functions)
VISION_OCR_ENABLED=true
VISION_OCR_MODE=document   # 'document' | 'text'
DEBUG_PERF=false

VITE_HABIT_CENTRAL_V1=1